{"ts":1367345575109,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"<!DOCTYPE html>\n<html>\n  <head>\n    <title>Election Data: <%=item.name%></title>\n    <link rel=\"stylesheet\" href=\"/style.css\">\n  </head>\n  <body>\n    <a href=\"/\"><h1>Taric Data</h1></a>\n    <dl>\n      <dt>Name</dt>\n      <dd><%=item.name%></dd>\n      <dt>Chapter</dt>\n      <dd><a href=\"/chapters/<%=item.chapter%>\"><%=item.chapter%></a></dd>\n      <% if (item.rule) { %>\n      <dt>Image URL</dt>\n      <dd><img src=\"<%=item.rule%>\" alt=\"<%=item.rule%>\"></dd>\n      <% } %>\n    </dl>\n    <div class=\"control\">\n      <h2>Update rule-name</h2>\n      <form method=\"post\" action=\"/rule-names/<%=item._id%>\">\n        <input type=\"hidden\" name=\"_method\" value=\"put\">\n        <!-- The line above tells the server to treat this as a PUT request. -->\n        <label>Name: \n          <input name=\"item[name]\" value=\"<%=item.name%>\" type=\"text\" required>\n        </label>\n        <label>Chapter: \n          <select name=\"item[Chapter]\" required>\n            <% related_items.forEach(function(related_item) { %>\n            <option value=\"<%=related_item._id%>\"\n              <% if (related_item._id === item.chapter) { %>selected<% } %>>\n              <%=related_item.name%>\n            </option>\n            <% }); %>\n          </select>\n        </label>\n        <label>Image URL:\n          <input name=\"item[rule]\" value=\"<%=item.rule%>\" type=\"url\" size=\"80\"> \n        </label>\n        <button>Update</button>\n      </form>\n    </div>\n  </body>\n</html>\n"]],"start1":0,"start2":0,"length1":0,"length2":1444}]],"length":1444}
